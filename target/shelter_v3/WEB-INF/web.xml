<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <servlet>
    <servlet-name>registration</servlet-name>
    <servlet-class>servlets.Registration</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>registration</servlet-name>
    <url-pattern>/reg</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>index</servlet-name>
    <servlet-class>servlets.Index</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>index</servlet-name>
    <url-pattern>/shelter</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>auth</servlet-name>
    <servlet-class>servlets.Authentication</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>auth</servlet-name>
    <url-pattern>/auth</url-pattern>
  </servlet-mapping>


    <servlet>
        <servlet-name>our_pets</servlet-name>
        <servlet-class>servlets.Our_pets</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>our_pets</servlet-name>
        <url-pattern>/our_pets</url-pattern>
    </servlet-mapping>

</web-app>
